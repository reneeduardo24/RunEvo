<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="gonzalez.hernandez.runevo">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_runevo_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_runevo_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RunEvo"
        tools:targetApi="31">
        <activity
            android:name=".Configuracion"
            android:exported="false" />
        <activity
            android:name=".ConfiguraPerfilActivity"
            android:exported="false" />
        <activity
            android:name=".RegistrarseActivity"
            android:exported="false" />
        <activity
            android:name=".iniciarSesion"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".FirstLayoutActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
ds        </activity>
    </application>

</manifest>